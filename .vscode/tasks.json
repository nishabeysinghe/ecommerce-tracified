{
	"version": "2.0.0",
    "command": "tsc",
    "isShellCommand": true,
    "args": [],
    "showOutput": "always",
    "tscCommand": true,
    "suppressTaskName": true,


"tasks": [
	{
		"type": "typescript",
		"tsconfig": "tsconfig.json",
		"option": "watch",
		"problemMatcher": [
			"$tsc-watch"
		]
	},
	{
		"taskName": "scripts",
		"problemMatcher": [
			"$gulp-tsc"
		]
	},
	{
		"taskName": "sass",
		"problemMatcher": [
			"$gulp-tsc"
		]
	},
	{
		"taskName": "watch1",
		"problemMatcher": [
			"$gulp-tsc"
		]
	}
]
}